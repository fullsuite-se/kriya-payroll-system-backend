model SssContributionRate {
    sss_contribution_rate_id String    @id @default(uuid()) @db.Char(36)
    salary_min               Decimal   @db.Decimal(10, 2)
    salary_max               Decimal   @db.Decimal(10, 2)
    employee_share           Decimal   @db.Decimal(10, 2)
    employer_share           Decimal   @db.Decimal(10, 2)
    total_contribution       Decimal   @db.Decimal(10, 2)
    effective_date_start     DateTime  @db.Date
    effective_date_end       DateTime? @db.Date
    created_at               DateTime  @default(now()) @db.Timestamp(0)
    updated_at               DateTime  @default(now()) @db.Timestamp(0)

    @@map("sss_contribution_rates")
}

model PhicContributionRate {
    phic_contribution_rate_id String    @id @default(uuid()) @db.Char(36)
    salary_min                Decimal   @db.Decimal(10, 2)
    salary_max                Decimal   @db.Decimal(10, 2)
    employee_share            Decimal   @db.Decimal(10, 2)
    employer_share            Decimal   @db.Decimal(10, 2)
    total_contribution        Decimal   @db.Decimal(10, 2)
    effective_date_start      DateTime  @db.Date
