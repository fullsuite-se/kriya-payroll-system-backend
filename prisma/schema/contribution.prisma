}

model HdmfContributionRate {
    hdmf_contribution_rate_id String    @id @default(uuid()) @db.Char(36)
    total_rate                Decimal   @db.Decimal(10, 2)
    employer_rate             Decimal   @db.Decimal(10, 2)
    employee_rate             Decimal   @db.Decimal(10, 2)
    effective_date_start      DateTime  @db.Date
    effective_date_end        DateTime? @db.Date
    created_at                DateTime  @default(now()) @db.Timestamp(0)
    updated_at                DateTime  @default(now()) @db.Timestamp(0)

    @@map("hdmf_contribution_rates")
}

model WithholdingTaxBracket {
    withholding_tax_id String       @id @default(uuid()) @db.Char(36)
    frequency          TaxFrequency
    bracket_number     Int
    min_compensation   Decimal      @db.Decimal(12, 2)
    max_compensation   Decimal      @db.Decimal(12, 2)
    base_tax           Decimal      @db.Decimal(12, 2)
    percent_over       Decimal      @db.Decimal(5, 2)
    excess_over        Decimal      @db.Decimal(12, 2)
    created_at         DateTime     @default(now()) @db.Timestamp(0)
    updated_at         DateTime     @default(now()) @db.Timestamp(0)

    @@map("withholding_tax_brackets")
}
